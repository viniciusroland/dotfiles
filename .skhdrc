cmd + alt - n : yabai -m space --create
cmd + alt - w : yabai -m space --destroy

alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east

ctrl - 1 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 1)
ctrl - 2 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 2)
ctrl - 3 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 3)
ctrl - 4 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 4)
ctrl - 5 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 5)
ctrl - 6 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 6)
ctrl - 7 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 7)
ctrl - 8 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 8)
ctrl - 9 : yabai -m space --focus $(yabai -m query --displays | python3 ~/Commands/yabai_spaces.py 9)
ctrl - 0 : yabai -m space --focus last
